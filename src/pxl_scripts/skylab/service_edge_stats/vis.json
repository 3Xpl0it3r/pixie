{
  "variables": [
    {
      "name": "start",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-10m"
    },
    {
      "name": "requesting_svc",
      "type": "PX_SERVICE",
      "description": "The full/partial name of the service to for request side. Format: ns/svc_name",
      "defaultValue": ""
    },
    {
      "name": "responding_svc",
      "type": "PX_SERVICE",
      "description": "The full/partial name of the service to for response side. Format: ns/svc_name",
      "defaultValue": ""
    }
  ],
  "globalFuncs": [
    {
      "outputName": "LET",
      "func": {
        "name": "svc_edge_let",
        "args": [
          {
            "name": "start",
            "variable": "start"
          },
          {
            "name": "requesting_svc",
            "variable": "requesting_svc"
          },
          {
            "name": "responding_svc",
            "variable": "responding_svc"
          }
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "Latency - p90",
      "position": {
        "x": 4,
        "y": 0,
        "w": 4,
        "h": 2
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "latency_p90",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "P90 Latency (ms)"
        },
        "xAxis": null
      }
    },
    {
      "name": "Requests per Second",
      "position": {
        "x": 0,
        "y": 0,
        "w": 4,
        "h": 2
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "rps",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "RPS"
        },
        "xAxis": null
      }
    },
    {
      "name": "Request Error Rate",
      "position": {
        "x": 8,
        "y": 0,
        "w": 4,
        "h": 2
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "error_rate",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "Error Rate (%)"
        },
        "xAxis": null
      }
    },
    {
      "name": "Summary",
      "position": {
        "x": 0,
        "y": 2,
        "w": 12,
        "h": 3
      },
      "func": {
        "name": "summary_edge_let",
        "args": [
          {
            "name": "start",
            "variable": "start"
          },
          {
            "name": "requesting_svc",
            "variable": "requesting_svc"
          },
          {
            "name": "responding_svc",
            "variable": "responding_svc"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.Table"
      }
    }
  ]
}
