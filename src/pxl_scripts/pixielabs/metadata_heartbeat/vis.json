{
  "variables": [
    {
      "name": "namespace",
      "type": "PX_NAMESPACE",
      "description": "The name of the namespace.",
      "defaultValue": "podspam"
    },
    {
      "name": "start",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-5m"
    }
  ],
  "globalFuncs": [
    {
      "outputName": "latencyTS",
      "func": {
        "name": "latency_quantiles_time_series",
        "args": [
          {
            "name": "start",
            "variable": "start"
          }
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "Function Latency",
      "position": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 3
      },
      "globalFuncOutputName": "latencyTS",
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "latency_p99",
            "mode": "MODE_LINE",
            "series": "id"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "latency (ms)"
        },
        "xAxis": null
      }
    },
    {
      "name": "Latency Overall",
      "position": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 6
      },
      "func": {
        "name": "latency_quantiles_overall",
        "args": [
          {
            "name": "start",
            "variable": "start"
          }
        ]
      },
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.Table"
      }
    },
    {
      "name": "PodGraph",
      "position": {
        "x": 0,
        "y": 3,
        "w": 6,
        "h": 3
      },
      "func": {
        "name": "pods_in_namespace",
        "args": [
          {
            "name": "namespace",
            "variable": "namespace"
          },
          {
            "name": "start",
            "variable": "start"
          }
        ]
      },
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "pod_count",
            "mode": "MODE_LINE",
            "series": "namespace"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "number of pods"
        },
        "xAxis": null
      }
    },
    {
      "name": "Raw data",
      "position": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 3
      },
      "func": {
        "name": "raw_table",
        "args": [
          {
            "name": "start",
            "variable": "start"
          }
        ]
      },
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.Table"
      }
    }
  ]
}
