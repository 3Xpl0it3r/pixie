{
  "variables": [
    {
      "name": "namespace",
      "type": "PX_NAMESPACE",
      "description": "The name of the namespace.",
      "defaultValue": "podspam"
    },
    {
      "name": "start",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-5m"
    }
  ],
  "globalFuncs": [],
  "widgets": [
    {
      "name": "PodGraph",
      "position": {
        "x": 0,
        "y": 3,
        "w": 6,
        "h": 3
      },
      "func": {
        "name": "pods_over_time",
        "args": [
          {
            "name": "namespace",
            "variable": "namespace"
          },
          {
            "name": "start",
            "variable": "start"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "pod_count",
            "mode": "MODE_LINE",
            "series": "namespace"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "number of pods"
        },
        "xAxis": null
      }
    }
  ]
}
