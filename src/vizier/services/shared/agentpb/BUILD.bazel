# gazelle:ignore
load("//bazel:proto_compile.bzl", "pl_cc_proto_library", "pl_go_proto_library", "pl_proto_library")

pl_proto_library(
    name = "agent_pl_proto",
    srcs = ["agent.proto"],
    visibility = ["//src/vizier:__subpackages__"],
    deps = [
        "//src/api/public/uuidpb:uuid_pl_proto",
        "@gogo_grpc_proto//github.com/gogo/protobuf/gogoproto:gogo_pl_proto",
    ],
)

pl_cc_proto_library(
    name = "agent_pl_cc_proto",
    proto = ":agent_pl_proto",
    visibility = ["//src/vizier:__subpackages__"],
    deps = [
        "//src/api/public/uuidpb:uuid_pl_cc_proto",
        "@gogo_grpc_proto//github.com/gogo/protobuf/gogoproto:gogo_pl_cc_proto",
    ],
)

pl_go_proto_library(
    name = "agent_pl_go_proto",
    importpath = "px.dev/pixie/src/vizier/services/shared/agentpb",
    proto = ":agent_pl_proto",
    visibility = ["//src/vizier:__subpackages__"],
    deps = [
        "//src/api/public/uuidpb:uuid_pl_go_proto",
    ],
)
