{
  "name": "pl_ui",
  "version": "0.0.1",
  "description": "Pixie Labs Interface Components",
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "@babel/preset-typescript": "^7.0.0-rc.2",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "node-sass": "^4.9.3",
    "react-dev-utils": "^5.0.1",
    "react-hot-loader": "^4.3.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.0",
    "webpack-archive-plugin": "^3.0.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "jest": {
    "globals": {
      "window": true
    },
    "setupFiles": [
      "<rootDir>/enzyme-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/config/jest/file-mock.js",
      "(\\.css|scss$)|(normalize.css/normalize)|(^exports-loader)": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(react-toolbox))"
    ],
    "testRegex": ".*-test\\.(ts|tsx|js)$",
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "suiteName": "Jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "material-components-web": "^0.38.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
